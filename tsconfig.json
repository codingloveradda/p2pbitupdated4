{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/pages/Form.tsx", "src/pages/_document.tsx", "src/pages/Homemain.tsx", "src/pages/index.jsx", "src/pages/_app.tsx", "src/pages/api/hello.tsx", "src/Actions/Actions.jsx", "src/Component/Accordion/Acorrdion.tsx"],
  "exclude": ["node_modules"]
}
